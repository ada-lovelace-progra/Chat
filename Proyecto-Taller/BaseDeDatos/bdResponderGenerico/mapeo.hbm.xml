<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">

<hibernate-mapping>

	<class name="bdResponderGenerico.RespuestaBD" table="Consultas">
		<id column="id" name="id" type="int" />
		<property column="clase" name="clase" type="string" />
		<property column="peticion" name="peticion" type="string" />
		<property column="respuesta" name="respuesta" type="string" />
	</class>

</hibernate-mapping>